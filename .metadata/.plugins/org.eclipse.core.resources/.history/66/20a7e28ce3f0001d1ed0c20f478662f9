management:
  endpoint:
    shutdown:
      enabled: true
  endpoints:
    web:
      base-path: /olx-monitor
      exposure:
        include: '*'
server:
  port: 9000
spring:
  application:
    name: advertise-service
  

eureka:
  client:
    register-with-eureka: true
    fetch-registry: true
  instance:
    hostname: localhost
    instance-id: ${spring.application.name}:${random.uuid}